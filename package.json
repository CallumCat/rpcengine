{
  "name": "rpcengine",
  "productName": "rpcengine",
  "version": "0.0.5",
  "description": "My Electron application description",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [
    "discord",
    "rpc",
    "electron"
  ],
  "author": "theqoobee",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theqoobee/rpcengine.git"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rpcengine"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "rpcengine",
            "certificateFile": "./cert.pfx",
            "certificatePassword": "password"
          }
        }
      ]
    }
  },
  "dependencies": {
    "discord-rpc": "^3.1.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.4.1",
    "popper.js": "^1.16.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.47",
    "@electron-forge/maker-deb": "^6.0.0-beta.47",
    "@electron-forge/maker-rpm": "^6.0.0-beta.47",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.47",
    "@electron-forge/maker-zip": "^6.0.0-beta.47",
    "decache": "^4.5.1",
    "electron": "7.1.8"
  },
  "bugs": {
    "url": "https://github.com/theqoobee/rpcengine/issues"
  },
  "homepage": "https://github.com/theqoobee/rpcengine#readme"
}
